<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <!-- Sequences for Travels Bookings -->
        <record id="seq_travels_bookings" model="ir.sequence">
            <field name="name">Bookings Sequence</field>
            <field name="code">travels.bookings.sequence</field>
            <field name="prefix">TB</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Sequences for Tour Packages -->
        <record id="seq_tour_packages" model="ir.sequence">
            <field name="name">Tour Package Sequence</field>
            <field name="code">tour.packages.sequence</field>
            <field name="prefix">TP</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>
</odoo>
